spawn telnet localhost 9025
expect "220 smtp.gomonar.ch ESMTP Monarch SMTP server ready."
send "EHLO localhost\r"
expect "250 SIZE 10485760"
send "MAIL FROM: <td512@td512.io>\r"
expect "250 Accepted"
send "RCPT TO: <td512@td512.io>\r"
expect "250 Accepted"
send "DATA\r"
expect "354 End data with <CR><LF>.<CR><LF>"
send "TEST6\r"
send ".\r"
expect "250 OK: message queued"
send "QUIT\r"
